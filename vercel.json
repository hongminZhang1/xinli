{
  "version": 2,
  "framework": "nextjs",
  "regions": ["hkg1"],
  "buildCommand": "npm run prisma:generate && npm run build",
  "installCommand": "npm install",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_PUBLIC_DATA_ACCESS_MODE": "api",
    "PRISMA_GENERATE_DATAPROXY": "true",
    "NODE_ENV": "production"
  }
}